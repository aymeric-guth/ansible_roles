---
static_host_map:
  '10.0.0.1': ['159.89.5.102:4242']

lighthouse:
  am_lighthouse: false
  interval: 60
  hosts:
    - '10.0.0.1'

pki:
  ca: {{ _nebula_dir}}/ca.crt
  cert: {{ _nebula_dir}}/host.crt
  key: {{ _nebula_dir}}/host.key

listen:
  host: 0.0.0.0
  port: 4242

firewall:
  outbound:
    - port: any
      proto: any
      host: any

  inbound:
    - port: any
      proto: any
      host: any
